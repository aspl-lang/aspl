[public]
function min(double a, double b) returns double{
	if(a < b){
		return a
	}
	return b
}

[public]
function max(double a, double b) returns double{
	if(a > b){
		return a
	}
	return b
}

[public]
function clamp(double value, double min, double max) returns double{
	if(value < min){
		return min
	}elseif(value > max){
		return max
	}
	return value
}