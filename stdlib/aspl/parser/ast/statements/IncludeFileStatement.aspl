import aspl.parser.ast
import aspl.parser.utils

[public]
class IncludeFileStatement extends Statement{

	[readpublic]
	property string file
	[readpublic]
	property IncludePhase phase
	
	method construct(string file, IncludePhase phase, Location? location){
        parent(Node).construct(location)
		this.file = file
		this.phase = phase
	}
	
}