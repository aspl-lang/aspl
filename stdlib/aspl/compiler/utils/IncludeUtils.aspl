import aspl.parser.utils

[public]
[static]
class IncludeUtils {

    [public]
    [static]
    [threadlocal]
    property map<string, IncludePhase> files

    [public]
    [static]
    method include(string file, IncludePhase phase) {
        if(files.containsKey(file)) {
            return
        }
        files[file] = phase
    }

}