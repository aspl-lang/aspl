[public]
class HttpResponse {

    [readpublic]
    property HttpResponseData data
    [readpublic]
    property map<string, list<string>> headers
    [readpublic]
    property int statusCode
    [readpublic]
    property string statusMessage
    [readpublic]
    property string httpVersion

    [public]
    method construct(HttpResponseData data, map<string, list<string>> headers, int statusCode, string statusMessage, string httpVersion) {
        this.data = data
        this.headers = headers
        this.statusCode = statusCode
        this.statusMessage = statusMessage
        this.httpVersion = httpVersion
    }

}